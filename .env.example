# Server Configuration
PORT=5001
COOKIE_KEY=your_cookie_secret_key_here

# Rate Limiting
WINDOW_SIZE=60
LIMIT_REQUESTS=100

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_USERNAME=default
REDIS_PASSWORD=your_redis_password_here

# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database_name

# GitHub App Configuration (Private Key - shared across all projects)
GITHUB_APP_PRIVATE_KEY=your_github_app_private_key_or_path_to_pem_file